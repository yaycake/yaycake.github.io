<div class="page-wrapper">

  <div class="contact-banner">
    <h2>Let's Chat.</h2>
    <h4>Shoot me a message here or on WeChat at @yay_cake.</h4>
  </div>
  <div class="contact-form">
    <form action="//formspree.io/email@domain.com" method="POST">
       <fieldset>
       <!-- <label for="name">Your name</label><br> -->
       <input type="text" name="name" placeholder="What's Your Name?" id="name" required>
       </fieldset>
       <fieldset>
       <!-- <label for="_replyto">Your email</label><br> -->
       <input type="email" name="_replyto" placeholder="your@email.com" id="_replyto" required>
       </fieldset>
       <fieldset>
       <!-- <label for="message">Your message</label><br> -->
       <textarea name="message" rows="1" placeholder="How can I help?" id="message" required></textarea>
       </fieldset>
       <input class="hidden" type="text" name="_gotcha" style="display:none">
       <input class="hidden" type="hidden" name="_subject" value="Message via http://domain.com">
      <input class="button submit" type="submit" value="Send">
    </form>
  </div>
</div>


